{
    "serverPort": 8080,
    "diagnosticPort": 8081,
    "logLevel": "info",
    "maxConnections": 100,
    "maxStackSize": 100,
    "staleConnectionPeriodSeconds":10,
    "logDirectory": "./logs",
    "logging": {
        "production" : {
            "logLevel":"error",
            "logConsole":false
        },
        "testing" : {
            "logLevel":"info",
            "logConsole":false
        },
        "development" : {
            "logLevel":"verbose",
            "logConsole":true
        },
        "debugging" : {
            "logLevel":"silly",
            "logConsole":true
        }
    }
}
